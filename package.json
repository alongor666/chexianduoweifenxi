    "preview": "npx serve out",
    "lint": "next lint",
    "test": "vitest -c configs/vitest.config.mts",
    "test:unit": "vitest -c configs/vitest.config.mts",
    "test:e2e": "playwright test",
    "test:upload": "node scripts/test_upload.js",
    "export": "NODE_OPTIONS='--max-old-space-size=2048' next build && next export",
    "deploy": "pnpm run build:static && touch out/.nojekyll",
    "deploy:preview": "pnpm run deploy && pnpm run preview",
